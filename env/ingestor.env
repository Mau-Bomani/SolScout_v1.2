# Redis Stream
STREAM_MARKET=soul.market.updates

# Concurrency
MAX_CONCURRENCY=8
REQUEST_TIMEOUT_MS=8000

# Timing
GLOBAL_TICK_SECONDS=60
BAR_INTERVAL_5M=300
BAR_INTERVAL_15M=900

# Retry settings
RETRY_BACKOFF_MS_MIN=500
RETRY_BACKOFF_MS_MAX=15000

# Cache
CACHE_TTL_SECONDS=600

# HTTP Server
LISTEN_ADDR=0.0.0.0
LISTEN_PORT=8082

# Service
SERVICE_NAME=ingestor
LOG_LEVEL=info
